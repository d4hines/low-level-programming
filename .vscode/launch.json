{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "by-gdb",
      "request": "launch",
      "name": "Launch(gdb)",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "cwd": "${workspaceRoot}"
    },
    {
      "name": "Launch LLDB",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/ch_2/print_rax",
      "preLaunchTask": "asm64"
    },
    {
      "type": "gdb",
      "request": "launch",
      "name": "GDB32",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "stopOnEntry": false,
      "preLaunchTask": "asm32"
    },
    {
      "type": "gdb",
      "request": "launch",
      "name": "GDB64",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "stopOnEntry": false,
      "preLaunchTask": "asm64"
    },
    {
      "type": "gdb",
      "request": "launch",
      "name": "GAS",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "stopOnEntry": false,
      "preLaunchTask": "gas"
    },
    {
      "type": "gdb",
      "request": "launch",
      "name": "GDB32+GCC",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "stopOnEntry": false,
      "preLaunchTask": "asm32+gcc"
    },
    {
      "type": "gdb",
      "request": "launch",
      "name": "GDB64+GCC",
      "program": "${fileDirname}/${fileBasenameNoExtension}",
      "stopOnEntry": false,
      "preLaunchTask": "asm64+gcc"
    }
  ]
}
